#
URxvt.font: xft:JetBrains Mono Medium:size=14:style=Regular, xft:symbola:size=14 URxvt*letterSpace: -1
urxvt*depth: 32
URxvt.scrollBar: false
!clickable urls
URxvt.url-launcher: /usr/bin/xdg-open

URxvt.intensityStyles: false
URxvt.transparent:   false
URxvt.depth: 24
 
Xft.dpi: 96
Xft.antialias: true
Xft.hinting: true
Xft.rgba: rgb
Xft.autohint: false
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault
 
URxvt.perl-ext-common: default,matcher,fullscreen,clipboard,resize-font
URxvt.matcher.button: 1
URxvt.keysym.F11: perl:fullscreen:switch
URxvt.secondaryWheel: 1
URxvt.internalBorder:  5
URxvt.keysym.Shift-Control-V: perl:clipboard:paste

URxvt.keysym.C-Up:     font-size:increase
URxvt.keysym.C-Down:   font-size:decrease
URxvt.keysym.C-S-Up:   font-size:incglobal
URxvt.keysym.C-S-Down: font-size:decglobal
URxvt.keysym.C-equal:  font-size:reset
URxvt.keysym.C-slash:  font-size:show

! rofi
rofi.lines: 5

rofi.font: SFProText Regular 15
rofi.fullscreen: false
rofi.bw: 1
rofi.eh: 2
rofi.separator-style: none
rofi.hide-scrollbar: true


! State:           'bg',     'fg',     'bgalt',  'hlbg',   'hlfg'
rofi.color-normal: #282A36,  #ffffff,  #282A36,  #FF79C6,  #FFFFFF

!                  'background', 'border'
rofi.color-window: #282A36,      #ff79c6
rofi.location:                       0
rofi.padding:                        20
rofi.levenshtein-sort:               true
rofi.case-sensitive:                 false
rofi.fuzzy:                          false
rofi.line-margin:                    2
rofi.markup-rows:                    false

! Dracula Xresources palette
*foreground: #F8F8F2
*background: #282A36
*color0:     #000000
*color8:     #4D4D4D
*color1:     #FF5555
*color9:     #FF6E67
*color2:     #50FA7B
*color10:    #5AF78E
*color3:     #F1FA8C
*color11:    #F4F99D
*color4:     #BD93F9
*color12:    #CAA9FA
*color5:     #FF79C6
*color13:    #FF92D0
*color6:     #8BE9FD
*color14:    #9AEDFE
*color7:     #BFBFBF
*color15:    #E6E6E6
