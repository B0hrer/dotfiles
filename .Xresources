
URxvt.font: xft:Hack:size=14:style=Regular, xft:symbola:size=14
URxvt*letterSpace: -1
urxvt*depth: 32
URxvt.scrollBar: false
!clickable urls
URxvt.url-launcher: /usr/bin/xdg-open

URxvt.intensityStyles: false
URxvt.transparent:   false
URxvt.depth: 24
 
Xft.dpi: 96
Xft.antialias: true
Xft.hinting: true
Xft.rgba: rgb
Xft.autohint: false
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault
 
URxvt.perl-ext-common: default,matcher,fullscreen,clipboard,resize-font
URxvt.matcher.button: 1
URxvt.keysym.F11: perl:fullscreen:switch
URxvt.secondaryWheel: 1
URxvt.internalBorder:  5
URxvt.keysym.Shift-Control-V: perl:clipboard:paste

URxvt.keysym.C-Up:     font-size:increase
URxvt.keysym.C-Down:   font-size:decrease
URxvt.keysym.C-S-Up:   font-size:incglobal
URxvt.keysym.C-S-Down: font-size:decglobal
URxvt.keysym.C-equal:  font-size:reset
URxvt.keysym.C-slash:  font-size:show

! rofi
rofi.lines: 5
rofi.eh: 2
rofi.padding: 200
rofi.fullscreen: false
rofi.bw: 0
rofi.separator-style: none
rofi.hide-scrollbar: true

rofi.color-enabled: true
rofi.color-window: argb:cc2f343f, argb:cc2f343f, argb:cc2f343f
rofi.color-normal: argb:002f343f, #d8dee8, argb:002f343f, argb:002f343f, #bf616a
rofi.color-active: #2f343f, #b48ead, #2f343f, #2f343f, #93e5cc
rofi.color-urgent: #2f343f, #ebcb8b, #2f343f, #2f343f, #ebcb8b

